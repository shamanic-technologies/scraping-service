{
  "name": "scraping-service",
  "version": "0.1.0",
  "description": "URL scraping service using Firecrawl - extracts company information",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:unit": "vitest run --dir tests/unit",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:watch": "vitest",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push"
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "drizzle-orm": "^0.36.0",
    "express": "^4.21.0",
    "postgres": "^3.4.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.0",
    "drizzle-kit": "^0.28.0",
    "supertest": "^7.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^2.0.0"
  }
}
